platform :ios, '11.2'

target 'littleprinter' do
  use_frameworks!

  pod 'SnapKit', :git => 'https://github.com/SnapKit/SnapKit.git', :branch => 'develop'
  pod 'HTMLString', :git => 'https://github.com/alexaubry/HTMLString.git', :branch => 'swift5'
end

target 'littleprintershare' do
    use_frameworks!
    pod 'HTMLString', :git => 'https://github.com/alexaubry/HTMLString.git', :branch => 'swift5'
end
